pipeline{
    agent any
    tools{
        maven 'maven3'
    }
    stages{
       stage('GetCode'){
            steps{
                    git branch: 'master', url: 'https://github.com/chintucool/jacoco-codecoverage.git'
                    echo "branch name - " + env.branch
            }
        }
       stage('SonarQube analysis') {
            when {
                    branch 'master'
            }
            steps{
                    sh """
                        java -version
                        mvn clean package sonar:sonar 
                    """
             }
        }
         
    }
}
